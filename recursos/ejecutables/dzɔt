verificar_key(){
$e "${amarillo}ingrese su key: "
$r key
PWD=$(pwd)
source ${PWD}/.key.log

if [[ "${key}" == "$form" || "${key}" == "${form}" ]]; then
        $e "\e[0;mVerificando key.." && sleep 3 && $e "${verde}[âœ“]Key vÃ¡lida.[âœ“]"
        $e "+REGISTRANDO SU IP EN LA BASE DE DATOS+"
        rm .key.log
        banner_drowkid;clear
elif [[ "${key}" == "-" || "${key}" == "${nk}" ]]; then
        $e "error"
else
        $e "Error"
fi
}
ofus () {
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
".")txt[$i]="+";;
"+")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="%";;
"%")txt[$i]="4";;
"-")txt[$i]="K";;
"K")txt[$i]="-";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}

genk(){
name="olal-dik"
code=$(openssl rand -base64 16)
key=$(ofus "@+&-$code+olal")
$e "$key"
$e "form='$key'">.key.log
}
home(){
termux(){
cd Install;bash SpaceNW1.sh
}
vps(){
wget https://raw.githubusercontent.com/drowkid-1/SpaceNetwork/main/Install/SpaceNW.sh
chmod 777 SpaceNW.sh;./SpaceNW.sh;
}
termux
}
################################################################################
############|ï½•ï½“ï½  ï½„ï½…  ï½ï½’ï½‡ï½•ï½ï½…ï½ï½”ï½ï½“|################################
