while :; do
  case $1 in
  --banner-drowkid | --banner-drowk | --banner-dk)
    banner_drowkid
    break
    ;;
  --banner-johan | --banner-jhn | --banner-j)
    banner_johan
    break
    ;;
  -g | --generar-key | --gen-k)
    genk
    break
    ;;
  -k | --ofus-key)
    verificar_key
    break
    ;;
  --inst)
    home --termux
    break
    ;;
  *) exit ;;
  esac
done
