verificar_key(){ 
$e "Aun en desarrollo"
#$e "${amarillo}ingrese su key: " && wget $IPC/.key && source ${PWD}/.key && $r key && PWD=$(pwd)
#if [[ "${key}" == "$form" || "${key}" == "${form}" ]]; then
#        $e "\e[0;mVerificando key.." && sleep 3 && $e "${verde}[âœ“]Key vÃ¡lida.[âœ“]" &&        $e "+REGISTRANDO SU IP EN LA BASE DE DATOS+" &&        rm .key.log &&        banner_drowkid;clear
#elif [[ "${key}" == "-" || "${key}" == "${nk}" ]]; then  
