tit_spnw() { $linea && $e "" && $e " 	           \033[1;97mğŸš€ |\e[33mï¼³ï½ï½ï½ƒï½…ï¼®ï½…ï½”ï½—ï½ï½’ï½‹\e[0;m| ğŸš€         " && $e "" && $linea ; }
tit_proto(){ $linea  &&  $e ""  && $e " 	           \033[1;97mğŸŒ |\e[33mï¼°ï½’ï½ï½”ï½ï¼ï¼­ï½ï½ï½ï½‡ï½…ï½’\e[0;m| ğŸŒ         " && $e "" && $linea ; }
