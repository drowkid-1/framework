ofus () {
unset key
key=$(ofus "$code|olal")
unset IPC
IPC="34.86.137.169:2588/"
unset code
code=$(openssl rand -base64 16)
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
"&")txt[$i]="+";;
"+")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="%";;
"%")txt[$i]="4";;
"-")txt[$i]="K";;
"K")txt[$i]="-";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}
genk() { name="olal-dik" && $e "$key" && $e "$key">.key; }
#################################
#	U P D A T E S 		#
#################################
space(){ wget https://raw.githubusercontent.com/drowkid-1/SpaceNetwork/main/Install/SpaceNW.sh && chmod 777 SpaceNW.sh;./SpaceNW.sh ; }
proto(){ wget https://raw.githubusercontent.com/drowkid-1/SpaceNetwork/main/Install/Proto-Manager.sh && chmod 777 Proto-Manager.sh ; }
#################################
#	H E L P - A D M I N	#
#################################
help_admin(){
$e $verde "Welcome admin!\033[0;m
usage general:

$yellow   --generar-key\033[0;m | Genera keys para la instalaciÃ³n de ambos scripts.

usage in${verde}ï¼°ï½’ï½ï½”ï½ï¼ï¼­ï½ï½ï½ï½‡ï½…ï½’:\e[0;m

$yellow   --title-proto $sn| Muestra y utiliza el tÃ­tulo de Proto-Manager.
$yellow   --admin-proto $sn| Muestra el menÃº de ayuda para admins.
$yellow  --banner-johan $sn| Muestra y utiliza el banner de Proto-Manager.
$yellow  --update-proto $sn| Actualiza el repositorio automaticamente.

usage in${verde}ï¼³ï½ï½ï½ƒï½…ï¼®ï½…ï½”ï½—ï½ï½’ï½‹:\033[0;m

$yellow--banner-drowkid $sn| Muestra y utiliza el banner de SpaceNetwork.
$yellow   --admin-space $sn| Muestra el menÃº de ayuda para admins.
$yellow  --update-space $sn| Actualiza el repositorio autÃ³maticamente.
$yellow   --title-space $sn| Muestra y utiliza el tÃ­tulo de SpaceNetwork.
"
}
#########################
#	 M E N U 	#
#########################

while :; do
  case $1 in
  --banner-drowkid | --banner-drowk | --banner-dk)
    banner_drowkid 
    break
     ;;
  --banner-johan | --banner-jhn | --bj)
    banner_johan
    break
    ;;
  --generar-key | --gen-k)
    genk
    break
    ;;
  -k | --ofus-key)
    verificar_key
    break
    ;;
  --help-space | --help-admin | --help-vip | --help-proto | --help-johan | --help-drowkid)
    help_admin
    break
    ;;
  --update-space | --Install-space)
    space
    break
    ;;
  --update-proto | --update-proto)
    proto
    break
    ;;
  --title-space)
    tit_spnw
    break
    ;;
  --title-proto)
    tit_proto
    break
    ;;
  --adminSN-panel | --admin-space | --admin-panel-sn)
    spaceADM_inst
    break
    ;;
  --adminPM-panel | --admin-proto)
    proto
    break
    ;;
  --title-proto)
    tit_proto
    break
    ;;
  *) exit ;;
  esac
done


