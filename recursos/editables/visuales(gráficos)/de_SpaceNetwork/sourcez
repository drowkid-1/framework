tit_spnw() { $linea && $e "" && $e " 	           \033[1;97m🚀 |\e[33mＳｐａｃｅＮｅｔｗｏｒｋ\e[0;m| 🚀         " && $e "" && $linea ; }
tit_proto(){ $linea  &&  $e ""  && $e " 	           \033[1;97m🌐 |\e[33mＰｒｏｔｏ－Ｍａｎａｇｅｒ\e[0;m| 🌐         " && $e "" && $linea ; }
