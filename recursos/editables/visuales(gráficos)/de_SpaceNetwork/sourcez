ofus () {
unset key
key=$(ofus "$code|olal")
unset IPC
IPC="34.86.137.169:2588/"
unset code
code=$(openssl rand -base64 16)
unset server
server=$(echo ${txt_ofuscatw}|cut -d':' -f1)
unset txtofus
number=$(expr length $1)
for((i=1; i<$number+1; i++)); do
txt[$i]=$(echo "$1" | cut -b $i)
case ${txt[$i]} in
"&")txt[$i]="+";;
"+")txt[$i]=".";;
"1")txt[$i]="@";;
"@")txt[$i]="1";;
"2")txt[$i]="?";;
"?")txt[$i]="2";;
"4")txt[$i]="%";;
"%")txt[$i]="4";;
"-")txt[$i]="K";;
"K")txt[$i]="-";;
esac
txtofus+="${txt[$i]}"
done
echo "$txtofus" | rev
}
genk() { name="olal-dik" && $e "$key" && $e "$key">.key; }
#################################
#	U P D A T E S 		#
#################################
space(){ wget https://raw.githubusercontent.com/drowkid-1/SpaceNetwork/main/Install/SpaceNW.sh && chmod 777 SpaceNW.sh;./SpaceNW.sh ; }
proto(){ wget https://raw.githubusercontent.com/drowkid-1/SpaceNetwork/main/Install/Proto-Manager.sh && chmod 777 Proto-Manager.sh ; }
##################################################
#		I N S T A L A C I O N		 #
##################################################
spaceADM_inst(){ cd Install;bash SpaceNW1.sh ; }
protoADM_inst(){ cd Install;bash Proto-Manager1.sh ; }
#########################
#	 M E N U 	#
#########################
while :; do
  case $1 in
  --banner-drowkid | --banner-drowk | --banner-dk)
    banner_drowkid 
    break
     ;;
  --banner-johan | --banner-jhn | --bj)
    banner_johan
    break
    ;;
  --generar-key | --gen-k)
    genk
    break
    ;;
  -k | --ofus-key)
    verificar_key
    break
    ;;
  --update-space | --update-sp)
    space
    break
    ;;
  --update-proto | --update-proto)
    proto
    break
    ;;
  --title-spacenw | --title-sn)
    tit_spnw
    break
    ;;
  --title-proto)
    tit_proto
    break
    ;;
  --adminSN-panel | --spacenw-adminpanel | --admin-panel-sn)
    spaceADM_inst
    break
    ;;
  --adminPM-panel | --admin-proto)
    protoADM_inst
    break
    ;;
  --title-proto)
    tit_proto
    break
    ;;
  *) exit ;;
  esac
done


